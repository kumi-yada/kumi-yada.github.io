{
  "version": 2,
  "projects": {
    "art-commission": {
      "root": "apps/art-commission",
      "sourceRoot": "apps/art-commission/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/art-commission",
            "index": "apps/art-commission/src/index.html",
            "baseHref": "/",
            "main": "apps/art-commission/src/main.tsx",
            "polyfills": "apps/art-commission/src/polyfills.ts",
            "tsConfig": "apps/art-commission/tsconfig.app.json",
            "assets": [
              "apps/art-commission/src/favicon.ico",
              "apps/art-commission/src/assets",
              "apps/art-commission/src/404.html"
            ],
            "styles": ["apps/art-commission/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "baseHref": "/art-commission/",
              "fileReplacements": [
                {
                  "replace": "apps/art-commission/src/environments/environment.ts",
                  "with": "apps/art-commission/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "art-commission:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "art-commission:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/art-commission/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/art-commission"],
          "options": {
            "jestConfig": "apps/art-commission/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "landing-page": {
      "root": "libs/landing-page",
      "sourceRoot": "libs/landing-page/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/landing-page/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/landing-page"],
          "options": {
            "jestConfig": "libs/landing-page/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "art-commission"
}
